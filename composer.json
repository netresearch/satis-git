{
    "name": "netresearch/satis-gitlab",
    "type": "project",
    "description": "composer/satis extended with the ability to generate SATIS configuration",
    "authors": [
        {
            "name": "MickaÃ«l BORNE",
            "email": "mborne@users.noreply.github.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MBO\\SatisGitlab\\": "src/MBO/SatisGitlab"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MBO\\SatisGit\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/satis-git"
    ],
    "require": {
        "ext-json": "*",
        "mborne/remote-git": "~0.2",
        "symfony/console": "^5.4",
        "composer/satis": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "php-coveralls/php-coveralls": "^2.5"
    },
    "config": {
        "allow-plugins": {
            "composer/satis": true
        }
    }
}
